{
  "lock": false,
  "nodeModulesDir": true,
  "tasks": {
    "start": "deno run -A --watch=static/,routes/,../src,../docs dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "imports": {
    "$fresh/": "../",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/@preact/signals-core@1.5.1",
    "$ga4": "https://raw.githubusercontent.com/denoland/ga4/main/mod.ts",
    "marked-mangle": "https://esm.sh/marked-mangle@1.1.7",
    "@std/path": "jsr:@std/path@0.216.0",
    "@std/regexp": "jsr:@std/regexp@0.216.0",
    "@std/assert": "jsr:@std/assert@0.216.0",
    "@std/html": "jsr:@std/html@0.216.0",
    "@std/front-matter": "jsr:@std/front-matter@0.216.0",
    "@std/semver": "jsr:@std/semver@0.216.0",
    "$fresh-testing-library": "https://deno.land/x/fresh_testing_library@0.8.0/mod.ts",
    "$fresh-testing-library/": "https://deno.land/x/fresh_testing_library@0.8.0/",
    "tailwindcss": "npm:tailwindcss@3.4.1",
    "tailwindcss/": "npm:/tailwindcss@3.4.1/",
    "tailwindcss/plugin": "npm:/tailwindcss@3.4.1/plugin.js"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
