{
  "lock": false,
  "tasks": {
    "test": "deno test -A --parallel --trace-ops",
    "fixture": "deno run -A --watch=static/,routes/ tests/fixture/dev.ts",
    "www": "deno task --cwd=www start",
    "screenshot": "deno run -A www/utils/screenshot.ts",
    "check:types": "deno check **/*.ts && deno check **/*.tsx",
    "ok": "deno fmt --check && deno lint && deno task check:types && deno task test",
    "install-puppeteer": "PUPPETEER_PRODUCT=chrome deno run -A --unstable https://deno.land/x/puppeteer@16.2.0/install.ts && PUPPETEER_PRODUCT=firefox deno run -A --unstable https://deno.land/x/puppeteer@16.2.0/install.ts",
    "test:www": "deno test -A tests/www/",
    "manifests": "deno run -A genAllManifest.ts"
  },
  "exclude": [
    "**/_fresh/*",
    "**/tmp/*"
  ],
  "imports": {
    "$fresh/": "./",
    "@std/path": "jsr:@std/path@0.216.0",
    "@std/regexp": "jsr:@std/regexp@0.216.0",
    "@std/assert": "jsr:@std/assert@0.216.0",
    "@std/async": "jsr:@std/async@0.216.0",
    "@std/html": "jsr:@std/html@0.216.0",
    "@std/front-matter": "jsr:@std/front-matter@0.216.0",
    "@std/semver": "jsr:@std/semver@0.216.0",
    "@std/testing": "jsr:@std/testing@0.216.0",
    "@std/fs": "jsr:@std/fs@0.216.0",
    "preact": "npm:preact@10.19.6",
    "@preact/signals-core@1.3.0": "npm:@preact/signals-core@1.3.0",
    "@preact/signals-core@1.2.3": "npm:@preact/signals-core@1.2.3",
    "@preact/signals-core": "npm:@preact/signals-core@1.5.1",
    "@preact/signals": "npm:@preact/signals@1.2.2",
    "preact-render-to-string": "npm:preact-render-to-string@6.4.0",
    "$fresh-testing-library": "https://deno.land/x/fresh_testing_library@0.11.1/mod.ts",
    "$fresh-testing-library/": "https://deno.land/x/fresh_testing_library@0.11.1/",
    "$ga4": "https://raw.githubusercontent.com/denoland/ga4/main/mod.ts",
    "marked-mangle": "npm:marked-mangle@1.0.1"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "lint": {
    "rules": { "exclude": ["no-window"] }
  }
}
